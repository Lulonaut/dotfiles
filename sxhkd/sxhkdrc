XF86AudioPlay
	playerctl play-pause

XF86AudioNext
	playerctl next

XF86AudioPrev
	playerctl previous

XF86AudioLowerVolume
	pactl set-sink-volume 0 -1%

XF86AudioRaiseVolume
	pactl set-sink-volume 0 +1%

ctrl + Print
	flameshot screen -c

super + e 
	alacritty --command lf

super + b
	$BROWSER

super + Return
	alacritty

super + g
	pcmanfm

XF86AudioMute
	pactl set-sink-mute 0 toggle

XF86Mail
	~/applications/bin/mail
